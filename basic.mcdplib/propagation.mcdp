mcdp {
    requires r_1 [Nat] 
    provides f_1 [Nat]
    variable v_1 [Nat]
    c_1 = Nat:1
    c_2 = c_1
    c_3 = c_2
    c_4 = c_3 + c_1

    r_2 = required r_1
    r_2b = required r_1

    f_2 = provided f_1
    f_2b = provided f_1

    requires r_5 = f_2b

    f3 = provided f_1 + f_2
    # v1 can be used as either
    r3 = r_2 + v_1
    f3b = f_2 + v_1
    f4 = provided f_1 + c_2
    r4 = required r_1 + c_2

    # These are really truly ambiguous
    # v2 = v1 + c1 
    v2 = v_1 + c_1  + f_2

   requires r_unconnected [Nat]
   provides f_unconnected [Nat]
}
